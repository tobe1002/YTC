package Chapter02;

public class Ex02_08 {
	public static void main(String[] args) {
		int a=3, b=3, c=3;
		a += 3;
		b *= 3;
		c %= 3;
		System.out.println("a=" + a + ", b=" + b + ", c=" + c);
		
		int d=3;
		d = d++;
		System.out.println("a=" + a + ", d=" + d);
		a = ++d;
		System.out.println("a=" + a + ", d=" + d);
		a = d--;
		System.out.println("a=" + a + ", d=" + d);
		a = --d;
		System.out.println("a=" + a + ", d=" + d);
	}
}
